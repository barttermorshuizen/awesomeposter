{
  "name": "@awesomeposter/db",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "gen": "drizzle-kit generate --config=./drizzle.config.ts",
    "push": "drizzle-kit push --config=./drizzle.config.ts",
    "build": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "pg": "^8.12.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.44.4"
  },
  "devDependencies": {
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.9.2",
    "@types/pg": "^8.11.6",
    "drizzle-orm": "^0.44.4"
  }
}
