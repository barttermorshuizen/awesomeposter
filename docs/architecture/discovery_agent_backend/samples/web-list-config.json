{
  "webList": {
    "list_container_selector": ".article-list",
    "item_selector": ".article-card",
    "fields": {
      "title": {
        "selector": ".article-card__title"
      },
      "excerpt": {
        "selector": ".article-card__summary"
      },
      "url": {
        "selector": "a.article-card__link",
        "attribute": "href"
      },
      "timestamp": {
        "selector": "time.published-at",
        "attribute": "datetime"
      }
    },
    "pagination": {
      "next_page": {
        "selector": "a.pagination__next",
        "attribute": "href"
      },
      "max_depth": 4
    }
  }
}
